    "vitest": "^4.0.16"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tsc-files --noEmit"
    ],
    "*.{js,jsx,ts,tsx}": [
      "oxlint --fix",
      "eslint --fix"
    ],
    "*.{json,md,yml}": [
      "prettier --write"
    ]
  }
