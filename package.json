  "files": [
    "dist",
    "README.zh-TW.md"
  ],
  "keywords": [],
  "author": "",
  "license": "MIT",
  "description": "",
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-oxlint": "^1.34.0",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "l": "^0.6.0",
    "lint-staged": "^16.2.7",
    "oxlint": "^1.34.0",
    "prettier": "^3.7.4",
    "tsc-files": "^1.1.4",
    "tsup": "^8.5.1",
